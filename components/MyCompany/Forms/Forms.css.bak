.boxArchivoSubmit {
  position: relative;
  top: -9px;
  width: 369px;
  height: 32px;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  margin-top: 20px;
}

.text {
  width: 203px;
  height: 15px;
  margin: 3px 17px 4px 0;
  font-family: Roboto;
  font-size: 13px;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
  color: #303030;
}

.boxSubmitArchivo {
  border: solid 0.8px #c7ccdc;
  width: 150px;
  height: 32px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-radius: 4px;
}

.buttonArchivo {
  width: 150px;
  height: 32px;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  font-family: Roboto;
  font-size: 13px;
  font-weight: 500;
  font-stretch: normal;
  font-style: normal;
  line-height: normal;
  letter-spacing: normal;
  color: #093c5d;
  border: none;
  background-color: transparent;
}
