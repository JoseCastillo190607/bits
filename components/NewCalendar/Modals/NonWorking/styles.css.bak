    margin-bottom: 15px;
  }

  .dayMonth{
    font-family: 'Roboto';
    font-style: normal;
    font-weight: 500;
    font-size: 13px;
    line-height: 15px;
    text-align: center;
    letter-spacing: 0.005em;

    color: #174767;
  }


  .containerDayMonth{
    margin-top: 5px;
    margin-bottom: 5px;
  }

  .inputSearcher {
    width: 100%;
    padding: 0px 0px;
    margin: 0px 0;
    box-sizing: border-box;
    border: 1px solid rgb(255,255,255);
    outline: none;
    background-image: url('/src/assets/svg/icono_buscar_tres.svg');
    background-repeat: no-repeat;
    background-position: calc(90%) 10px; 

  }

  .inputSearcher:focus{
    width: 100%;
    padding: 0px 0px;
    margin: 0px 0;
    box-sizing: border-box;
    border: 1px solid rgb(255,255,255);
    outline: none;
    padding: 0px 10px
  }

  .inputFilter {
    width: 100%;
    padding: 0px 0px;
    margin: 0px 0;
    box-sizing: border-box;
    border: 1px solid rgb(255,255,255);
    outline: none;
    background-repeat: no-repeat;
    background-position: calc(90%) 10px; 
    background-image: url('/src/assets/svg/icono_Path.svg');
  }

  .inputFilter:focus{
    width: 100%;
    padding: 0px 0px;
    margin: 0px 0;
    box-sizing: border-box;
    border: 1px solid rgb(255,255,255);
    outline: none;
  }

